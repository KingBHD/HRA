{
  "compileOnSave": true,
  "compilerOptions": {
    "sourceMap": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "lib": ["esnext"],
    "module": "CommonJS",
    "noImplicitAny": false,
    "outDir": "dist",
    "resolveJsonModule": true,
    "strict": true,
    "strictNullChecks": false,
    "target": "ES6",
    "watch": true
  },
  "include": ["src"]
}
